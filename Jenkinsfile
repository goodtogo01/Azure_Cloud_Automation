pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build the Project'
            }
        }
		stage('Test') {
            steps {
                echo 'Test is completed Ready to deploy'
            }
        }
		stage('Deploy') {
            steps {
                echo 'Successfully Deployed to the end user'
            }
        }

    }
}
